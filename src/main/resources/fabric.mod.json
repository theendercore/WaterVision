{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Water Vision",
  "description": "See in water!",
  "authors": [
    "Ender"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/water_vision/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.theendercore.water_vision.WaterVisionKt::onInitialize"
      }
    ],
    "modmenu": [
      "com.theendercore.water_vision.config.ModMenuCompat"
    ]
  },
  "mixins": [
    "water_vision.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.14.12",
    "fabric": "*",
    "minecraft": "1.20.x",
    "java": ">=17",
    "fabric-language-kotlin": ">=1.9.6+kotlin.1.8.22"
  },
  "suggests": {
    "another-mod": "*"
  }
}
